<%= form_for @location do |f| %>

  <fieldset>
    <%= f.label :address, 'Address' %>
    <%= f.text_field :address %>
  </fieldset>
  <fieldset>
    <%= f.label :lat, 'Latitude' %>
    <%= f.text_field :lat %>
  </fieldset>
  <fieldset>
    <%= f.label :long, 'Longitude' %>
    <%= f.text_field :long %>
  </fieldset>
  

  <%= f.submit 'Add Location'%>
<% end %>